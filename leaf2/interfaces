# This file describes the network interfaces available on your system
# and how to activate them. For more information, see interfaces(5).

source /etc/network/interfaces.d/*.intf

# The loopback network interface
auto lo
iface lo inet loopback
    address 10.2.2.2/32

# The primary network interface
#auto eth0
#iface eth0 inet dhcp

auto swp1
iface swp1
    address 10.2.2.2/32

auto swp2
iface swp2
    address 10.2.2.2/32

auto swp3
iface swp3
    address 10.2.2.2/32

auto swp4
iface swp4
    address 10.2.2.2/32

auto swp5
iface swp5
    address 10.2.2.2/32

auto swp6
iface swp6
    address 10.2.2.2/32

auto fabric
iface fabric
